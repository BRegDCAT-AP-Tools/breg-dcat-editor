<script type="text/javascript">

    /*add dcsMultiTrack for search in euvoc home page*/
    document.querySelector(".main-search-form.euvoc-search-form form").addEventListener("submit", function(){

        var euroVocOnly = document.querySelector(".main-search-form.euvoc-search-form form .checkbox input");
        var euroVocOnlyValue = (euroVocOnly && euroVocOnly.value === "true" ? 1 : 0);

        var mainSearchField = document.querySelector(".main-search-form.euvoc-search-form form input.lfr-search-keywords");
        var mainSearchFieldValue = mainSearchField.value;

        dcsMultiTrack(
            'WT.dl',               '99',
            'WT.oss',              ""+mainSearchFieldValue,
            'DCSext.w_oss_eurovoc',    ""+euroVocOnlyValue,
            'DCSext.w_oss_origin',        'Search Home Page');
    });

    /*add dcsMultiTrack for search in euvoc home page with sparql*/
    sparqlSearchMainSearchEuVocHome = function(){
        dcsMultiTrack(
            'WT.dl',               '99',
            'DCSext.w_oss_origin',        'SPARQL Search');
    };


    var getEuvocSearchQueryString = function (query) {
        var queryString='';
        AUI().use('node', function (Y) {
            var crtNamespace = '_eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC_';
            var searchEuvocOnly = Y.one('#_eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC_euvocSearchOnly').get('value');
            queryString= '&'.concat(crtNamespace).concat('euvocOnly=').concat(searchEuvocOnly)
                .concat('&').concat(crtNamespace).concat('keywords=').concat(query);
        });
        return queryString;
    };

    AUI().ready('datasource-io', 'autocomplete-list', 'aui-base', 'aui-io-request', 'autocomplete-filters', 'autocomplete-highlighters', function (A) {

        var searchInputId= "queryText";
        var namespaceStr = '_eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC_';
        initFields(namespaceStr, searchInputId);

        var datasource = new A.DataSource.IO({
            source: 'https://op.europa.eu/en/web/eu-vocabularies/home?p_p_id=eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage'
        });
        var autoComplete = new A.AutoCompleteList({
            allowBrowserAutocomplete: false,
            activateFirstItem: false,
            inputNode: '#_eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC_queryText',
            maxResults: 10,
            render: true,
            source: datasource,
            minQueryLength:2,
            queryDely: 1,
            align: {
                node: '#_eu_europa_publications_portlet_euvocsearch_EuvocSearchPortlet_INSTANCE_RfAvNCqylRfC_queryText',
                points: ['tr', 'br']
            },
            requestTemplate: getEuvocSearchQueryString,
            resultListLocator: function (response) {
                var responseData = A.JSON.parse(response[0].responseText);
                return responseData;
            },
            width: 400
        });
    });
</script>


	
	
					
				
			
		
	
	


