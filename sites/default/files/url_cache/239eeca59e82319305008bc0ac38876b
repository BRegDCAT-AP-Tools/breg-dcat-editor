<script type="text/javascript" src="/o/eu.europa.publications.portlet.euvocheadersearch/js/euVocHeaderSearch.js?t=2020-08-31T16:24:02Z"></script>
<script type="text/javascript">

    /*add dcsMultiTrack for search in the header*/
    var element = document.querySelector("#mainSearchStaticOption form");
    if (element) {
        element.addEventListener("submit", function () {

            var dropdownField = document.querySelector("#mainSearchStaticOption form select");
            var dropdownFieldText = dropdownField.options[dropdownField.selectedIndex].text;

            var searchField = document.querySelector("#mainSearchStaticOption form input.searchinput");
            var searchFieldValue = searchField.value;

            dcsMultiTrack(
                    'WT.dl', '99',
                    'WT.oss', searchFieldValue,
                    'DCSext.w_oss_dropdown', dropdownFieldText,
                    'DCSext.w_oss_origin', 'Search Header');
        });
    }

    /*hide main search*/
    window.onload = function () {
        hideMainSearch();
    };
    window.onresize = function() {
        hideMainSearch();
    };

    hideMainSearch = function(){
        if(document.querySelector("#headercontent") != null){
            if (window.innerWidth <= 991){
                if((document.querySelector("#mainSearchStatic") != null)){
                    document.querySelector("#mainSearchStatic" ).classList.add('hidden');
                }
            }else{
                if((document.querySelector("#mainSearchStatic") != null)){
                    document.querySelector("#mainSearchStatic" ).classList.remove('hidden');
                }
            }
        }
    };


    var getEuvocHeaderSearchQueryString = function (query) {
        var queryString = '';
        AUI().use('node', function (Y) {
            var crtNamespace = '_eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI_';
            var selectObj = Y.one('#' + crtNamespace + 'euvocHeaderSearchSelect');
            var selectedOptionValue = selectObj.get(selectObj.get('selectedIndex')).get('value');
            queryString = '&'.concat(crtNamespace).concat('searchScopeValue=').concat(selectedOptionValue)
                    .concat('&').concat(crtNamespace).concat('keywords=').concat(query);
        });
        return queryString;
    };

   /* var euVocHeaderSearch_loadSecondJavascript = true;
    var euVocHeaderSearchSearchInputId = "queryText";
    var euVocHeaderSearch_portletNamespace = '_eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI_';*/

    AUI().ready('datasource-io', 'autocomplete-list', 'aui-base', 'aui-io-request', 'autocomplete-filters', 'autocomplete-highlighters', function (A) {

        var searchInputId= "queryText";
        var namespaceStr = '_eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI_';
        init(namespaceStr, searchInputId);


        var datasource = new A.DataSource.IO({
            source: 'https://op.europa.eu/en/web/eu-vocabularies/home?p_p_id=eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage'
        });
        var autoComplete = new A.AutoCompleteList({
            allowBrowserAutocomplete: false,
            activateFirstItem: false,
            inputNode: '#_eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI_queryText',
            maxResults: 10,
            render: true,
            source: datasource,
            minQueryLength: 2,
            align: {
                node: '#_eu_europa_publications_portlet_euvocheadersearch_EuVocHeaderSearchPortlet_INSTANCE_cgvN1i7BbgdI_queryText',
                points: ['tr', 'br']
            },
            requestTemplate: getEuvocHeaderSearchQueryString,
            resultListLocator: function (response) {
                var responseData = A.JSON.parse(response[0].responseText);
                return responseData;
            }
        });
    });
</script>

	
	
					
				
			
		
	
	


