    AUI().use("webtrends", function (A) {
        var wtsin = A.one('meta[name=WT.si_n]') ? A.one('meta[name=WT.si_n]').get('content') : '';
        if (A.WebTrends) {
            _webtrends = A.WebTrends({
                dcsid: 'dcs73pc3h10000s189bpw2741_4b6l',
                domain: 'sdc-op.mainstrat.com',
                cookieDomain: 'op.europa.eu'

            });
            _webtrends.dcsGetId();
            _webtrends.dcsCustom = function () {
                // add here custom parameters
            }
            _webtrends.dcsCollect();
            A.all('a.download').on('click', function (e) {
                var node = A.one(e.currentTarget),
                        dataUri = node.getAttribute('data-uri'),
                        dataFormat = node.getAttribute('data-format'),
                        dataLanguage = node.getAttribute('data-language'),
                        dataId = node.getAttribute('data-id'),
                        dataAuthor = node.getAttribute('data-publication-author'),
                        dataCatalogueNumber = node.getAttribute('data-publication-catalogueNumber'),
                        dataTitle = node.getAttribute('data-publication-title'),
                        dataCollection = node.getAttribute('data-publication-collection'),
                        wtPathParameter = node.getAttribute('webtrends-pathParameter'),
                        wtPathValue = node.getAttribute('webtrends-pathValue');
                e.preventDefault();
                if (dataUri && dataFormat) {
                    A.WebTrends().dcsMultiTrack('WT.dl', '99',
                            'DCSext.w_doc_download', '1',
                            'DCSext.w_doc_format', dataFormat,
                            'DCSext.w_oss_location', wtPathParameter,
                            'DCSext.w_oss_click', dataFormat,
                            'WT.si_n', wtsin,
                            'WT.si_p', wtPathValue,
                            "WT.z_docID", dataId,
                            "WT.z_docLanguage", dataLanguage,
                            "WT.z_docFormat", dataFormat,
                            "WT.publicationAuthor", dataAuthor,
                            "WT.cellarId", dataId,
                            "WT.w_doc_catalogue_number", dataCatalogueNumber,
                            "WT.w_doc_title", dataTitle,
                            "DCSext.w_collection_etype", dataCollection
                    );
                }
                else {
                    if (!dataUri) {
                        dataUri = node.getAttribute('href');
                    }
                }
                window.location.href = dataUri;
            });
        }

        A.all('a.documentDetailLink').on('click', function (e) {
            var node = A.one(e.currentTarget),
                    dataUri = node.getAttribute('data-uri'),
                    dataFormat = node.getAttribute('data-format');
            if (dataUri) {
                A.WebTrends().dcsMultiTrack('WT.dl', '21',
                        'DCSext.w_oss_click', 'Publication Detail',
                        'WT.si_n', wtsin,
                        'WT.si_p', 'Publication Detail'
                );
            }

        });


    });
